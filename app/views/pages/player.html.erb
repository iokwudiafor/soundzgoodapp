<h1> Play your favorite tracks!</h1>

<section id="audio"> 
</section>
 
 $(document).ready(function() {
    var audioSection = $('section#audio');
    $('a.music_player').click(function() {
 
        var audio = $('<audio>', {
             controls : 'controls'
        });
 
        var url = $(this).attr('href');
        $('<source>').attr('src', url).appendTo(audio);
        audioSection.html(audio);
        return false;
        });
    });
 
 
 <table>
<% @music.each do |song| %>
    <tr>
        <td><%= song.key %></td>
        <td><%= link_to "Your Music", download_url_for(song.key), :class => "music_player" %></td>
    </tr>
<% end %>
</table>